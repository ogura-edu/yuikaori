<h1>Editing Picture</h1>

<div class="left">
  <%= image_tag(@picture.address) %>
</div>

<div class="right">
  <%= render 'form', picture: @picture %>
</div>

<div class="footer">
  <%= link_to 'Back', pictures_path, class:"link" %>
  <%= link_to 'Show', picture_path(@picture), class:"link" %>
</div>
